generic:
  role_description: |
    Role to open firewall ports for incoming traffic.
    Supports firewalld, ufw and iptables
github:
  fail_fast: false
molecule:
  prepare:
    vars:
      prepare_firewall: true
  converge:
    vars:
      firewall_ports:
        - port: "22"
          proto: tcp
        - port: "53"
          proto: udp
